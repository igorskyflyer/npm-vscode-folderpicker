{
	"include": [
		"./src/*.js"
	],
	"exclude": [
		"node_modules"
	],
	"compilerOptions": {
		"allowJs": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"outDir": "dist",
		"alwaysStrict": true,
		"listEmittedFiles": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"pretty": true,
	}
}
